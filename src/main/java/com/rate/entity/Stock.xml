    <attribute name="name" type="java.lang.String" column="NAME" resource="true" size="128"/>
    <attribute name="stockId" type="java.lang.Integer" column="STOCK_ID"  persistence="no"/>
    <attribute name="price" type="java.lang.Double" column="PRICE"  persistence="no"/>
    <attribute name="volume" type="java.lang.Double" column="VOLUME"  persistence="no"/>
